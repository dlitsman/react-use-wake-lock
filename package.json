  "bugs": {
    "url": "https://github.com/dlitsman/react-wake-lock/issues"
  },
  "homepage": "https://github.com/dlitsman/react-wake-lock#readme",
  "devDependencies": {
    "@types/react": "^18.2.15",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-plugin-dts": "^3.4.0"
  }
}
