  "description": "React hooks to make it easier to use Screen Wake Lock API",
  "main": "./dist/react-use-wake-lock.umd.cjs",
  "module": "./dist/react-use-wake-lock.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-use-wake-lock.js",
